Grammar

    0 $accept: program $end

    1 $@1: ε

    2 program: "start" T_id $@1 stmts "end"

    3 stmts: '(' stmt ')' stmts
    4      | ε

    5 stmt: printcmd
    6     | asmt

    7 printcmd: "print" expr

    8 asmt: '=' T_id expr

    9 expr: T_num
   10     | T_real
   11     | T_id
   12     | T_type expr
   13     | '+' expr expr
   14     | '*' expr expr
   15     | "+." expr expr
   16     | "*." expr expr
   17     | '(' expr ')'


